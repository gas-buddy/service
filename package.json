{
  "name": "@gasbuddy/service",
  "version": "5.1.0",
  "description": "An opinionated framework for building configuration driven services - web, api, or job. Uses swagger, winston, express, confit, ES7 and tap.",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register tests/test_*.js",
    "cover": "cross-env BABEL_ENV=test nyc tap tests/test_*.js && nyc check-coverage && nyc report --reporter=text",
    "ci-test": "cross-env NODE_ENV=test nyc tap -Rtap tests/test_*.js | tee .tap_output | tap -Rspec -",
    "ci-cover": "nyc check-coverage",
    "ci-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "build": "babel src/ --out-dir build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gas-buddy/service.git"
  },
  "bin": {
    "start-server": "build/start-server.js"
  },
  "engines": {
    "node": ">=8.9"
  },
  "keywords": [
    "gasbuddy",
    "service",
    "swagger",
    "winston",
    "express",
    "confit",
    "babel",
    "es7",
    "tap"
  ],
  "author": "GasBuddy <developers@gasbuddy.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gas-buddy/service/issues"
  },
  "homepage": "https://github.com/gas-buddy/service#readme",
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 45,
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov"
    ]
  },
  "dependencies": {
    "@gasbuddy/composert": "^1.0.0",
    "@gasbuddy/configured-prometheus-client": "^1.4.2",
    "@gasbuddy/configured-swagger-client": "^2.0.0",
    "@gasbuddy/configured-swaggerize-express": "^1.5.0",
    "@gasbuddy/configured-winston": "^1.7.0",
    "@gasbuddy/configured-winston-logstash": "^2.0.0",
    "@gasbuddy/express-promise-patch": "^1.0.3",
    "@gasbuddy/hydration": "^1.2.2",
    "@gasbuddy/kms-crypto": "^1.2.1",
    "@gasbuddy/meddleware": "^1.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bson-objectid": "^1.2.1",
    "confit": "^2.1.0",
    "express": "^4.16.2",
    "express-enrouten": "^1.3.0",
    "minimist": "^1.2.0",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1",
    "shortstop-dns": "^1.0.0",
    "shortstop-handlers": "^1.0.0",
    "shortstop-yaml": "^1.0.0",
    "source-map-support": "^0.5.0",
    "superagent": "^3.8.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-gasbuddy": "^3.1.0",
    "cross-env": "^5.1.1",
    "eslint-config-gasbuddy": "^4",
    "supertest": "^3.0.0",
    "tap": "^11.0.0",
    "tap-markdown": "^1.2.1"
  }
}
